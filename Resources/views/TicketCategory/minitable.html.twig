<table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>#</th>
        <th>Nom</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for category in ticketCategoryList %}
        <tr>
            <td>{{ loop.index }}</td>
            <td>{{ category.name }}</td>
            <td>
                <div class="btn-group btn-group-xs">
                    <a href="{{ path('ticket_category_update', {id: category.id}) }}" class="btn btn-default"><i class="fa fa-pencil"></i> </a>
                </div>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>